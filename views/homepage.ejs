<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Home</title>
    <!--<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css">-->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <!-- Latest compiled JavaScript -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js" integrity="sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T"
        crossorigin="anonymous"></script>
    <!--<script src="https://chatchitvn.herokuapp.com/socket.io/socket.io.js"></script>-->
    <script src="/socket.io/socket.io.js"></script>
    <script src="exec.js"> </script>
    <link rel="stylesheet" type="text/css" href="layout.css" />
</head>

<body>
    <div id="container-fluid">
        <div id="txtGroup">
            <div id="msg"></div>
            <div id="backLogin">
                <a href="https://chatchitvn.herokuapp.com/">click here</a> to back to login</div>
        </div>
        <div id="ctrl-form">
           <div class="text-title"><h4>Enter username and password</h4></div>
            <input type="text" class="form-control" id="eml" placeholder="Email" />
            <br>
            <input type="text" class="form-control" id="usr" placeholder="Username" />
            <br>
            <input type="password" class="form-control" id="pwd" placeholder="Password" />
            <div class="no-account">No account? <a class="register">Create one!</a></div>
            <div id="txtSgFail">Incorrect user ID or password. Type the correct user ID and password, and try again.</div>
            <br>
            <div id="btnGroup">
                <button type="button" class="btn btn-info" id="btnSignIn">Sign In</button>
                <button type="button" class="btn btn-info" id="btnSignUp">Sign Up</button>
                <button type="button" class="btn btn-info" id="btnReg">Register</button>
                <button type="button" class="btn btn-info" id="btnAdd">Add</button>
            </div>
        </div>
        <div id="chatForm">
            <div id="left">
                <div id="me-chatvn">
                    
                    <div id="me-infor">
                        <img class="avatar" src="https://avatar.skype.com/v1/avatars/pham.nghiep87?auth_key=-1911296691&returnDefaultImage=false&cacheHeaders=true">
                       <!--<div class="presence-status">   
                    </div> -->
                        <div id="displayname">User's Online</div>
                        <div id="status">Online</div>
                    </div>
                    <div id="container">
                       <div class="glyphicon glyphicon-search"></div>   
                       <div type="text" class="search" placeholder="Search"></div>
                    </div>
                </div>
                <div  id="menubar">   
                    <span class="addfrd">
                            <div class="icon-add-friend"> <span class="glyphicon glyphicon-plus"></div>
                     </span>                    
                            
                </div>
                <div id="separator"><span class="fav">Favorites</span></div>
                <div id="listFriend">
                </div>

            </div>
            <div id="right">
                <div id="listMessage">
                </div>
                <div id="annoucement"></div>
                <div id="txtArea">
                    <input type="text" id="msgBox" placeholder="Type a message here" />
                </div>
            </div>
        </div>
    </div>

</body>

</html>